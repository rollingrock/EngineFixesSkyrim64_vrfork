{
  "configurePresets": [
    {
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": {
          "type": "STRING",
          "value": "Debug"
        },
        "CMAKE_INTERPROCEDURAL_OPTIMIZATION": {
          "type": "BOOL",
          "value": "ON"
        },
        "CMAKE_INTERPROCEDURAL_OPTIMIZATION_DEBUG": {
          "type": "BOOL",
          "value": "OFF"
        },
        "CMAKE_CXX_FLAGS": "/EHsc /MP /W4 /wd4996 /wd5082 /external:anglebrackets /external:W0 $penv{CXXFLAGS}"
      },
      "errors": {
        "deprecated": true
      },
      "hidden": true,
      "name": "cmake-dev",
      "warnings": {
        "deprecated": true,
        "dev": true
      }
    },
	{
      "environment": {
        "CXXFLAGS": "/MP /W4 /WX /external:anglebrackets /external:W0 $penv{CXXFLAGS}"
      },
      "generator": "Visual Studio 17 2022",
      "hidden": true,
      "toolset": "v143",
      "name": "vs2022"
    },
    {
      "name": "se",
      "hidden": true,
      "binaryDir": "${sourceDir}/build"
    },
    {
      "name": "ae",
      "hidden": true,
      "binaryDir": "${sourceDir}/buildae",
      "cacheVariables": {
        "BUILD_SKYRIMAE": true
      }
    },
    {
      "name": "vr",
      "hidden": true,
      "binaryDir": "${sourceDir}/buildvr",
      "cacheVariables": {
        "BUILD_SKYRIMVR": true
      }
    },
    {
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": {
          "type": "STRING",
          "value": "$env{VCPKG_INSTALLATION_ROOT}/scripts/buildsystems/vcpkg.cmake"
        }
      },
      "hidden": true,
      "name": "vcpkg"
    },
    {
      "cacheVariables": {
        "Boost_USE_STATIC_LIBS": {
          "type": "BOOL",
          "value": "ON"
        },
        "Boost_USE_STATIC_RUNTIME": {
          "type": "BOOL",
          "value": "OFF"
        },
        "CMAKE_MSVC_RUNTIME_LIBRARY": {
          "type": "STRING",
          "value": "MultiThreaded$<$<CONFIG:Debug>:Debug>DLL"
        },
        "VCPKG_OVERLAY_PORTS": {
          "type": "STRING",
          "value": "${sourceDir}/cmake/ports"
        },
        "VCPKG_OVERLAY_TRIPLETS": {
          "type": "STRING",
          "value": "${sourceDir}/cmake/triplets"
        },
        "VCPKG_TARGET_TRIPLET": {
          "type": "STRING",
          "value": "x64-windows-custom"
        }
      },
      "hidden": true,
      "name": "windows"
    },
	{
      "description": "VS2022/Windows/VCPkg",
      "inherits": [
        "cmake-dev",
        "vcpkg",
        "vs2022",
        "se",
        "windows"
      ],
      "name": "vs2022-windows-vcpkg"
    },
    {
      "name": "vs2022-windows-vcpkg-ae",
      "inherits": [
        "cmake-dev",
        "vcpkg",
        "windows",
        "vs2022",
        "ae"
      ]
    },
    {
      "name": "vs2022-windows-vcpkg-vr",
      "inherits": [
        "cmake-dev",
        "vcpkg",
        "windows",
        "vs2022",
        "vr"
      ]
    }
  ],
  "version": 2
}
